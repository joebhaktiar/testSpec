{
  "name": "twe",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "build:css": "postcss styles/tailwind.css -o styles/index.css",
    "build:css:purge": "cross-env NODE_ENV=production npm run build:css",
    "dev": "npm run build:css && next dev -p 3900",
    "start": "npm run build:css && next dev -p 3900",
    "build": "cross-env npm run build:css:purge && next build && next export",
    "build:export": "cross-env npm run build:css:purge && next export",
    "start:prod": "npx next start -p 3900",
    "set:dev": "cp templates/twe/stage/dev.js templates/twe/stage/index.js",
    "set:sit": "cp templates/twe/stage/sit.js templates/twe/stage/index.js",
    "set:eite": "cp templates/twe/stage/eite.js templates/twe/stage/index.js",
    "set:pete": "cp templates/twe/stage/pete.js templates/twe/stage/index.js",
    "set:prod": "cp templates/twe/stage/prod.js templates/twe/stage/index.js",
    "deploy:dev": "npm run set:dev && npm run build && sls deploy && npm run build:css",
    "deploy:devFront": "npm run set:dev && npm run build && sls deploy && npm run build:css",
    "deploy:sitFront": "npm run set:sit && npm run build && sls deploy --stage sit && npm run build:css",
    "deploy:eiteFront": "npm run set:eite && npm run build && sls deploy --stage eite && npm run build:css",
    "deploy:peteFront": "npm run set:pete && npm run build && sls deploy --stage pete && npm run build:css",
    "cypress:open": "cypress open",
    "cypress:suite": "cypress run \"cypress/integration/**/*\"",
    "cypress:functional": "cypress run --spec \"cypress/integration/functional/*.spec.js\"",
    "cypress:global": "cypress run --spec \"cypress/integration/global/*.spec.js\"",
    "cypress:supplemental": "cypress run --spec \"cypress/integration/supplemental/*.spec.js\"",
    "cypress:regression": "cypress run --spec \"cypress/integration/supplemental/regression.spec.js\"",
    "cypress:sanity": "cypress run --spec \"cypress/integration/supplemental/sanity.spec.js\"",
    "test": "npx cypress run -s cypress/integration/twe/calc.spec.js --reporter junit",
    "jest": "jest --watch"
  },
